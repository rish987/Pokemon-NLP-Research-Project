            a.id = 'amazon-related';
            var linkText = document.createTextNode("Bulba Readership Survey - Win up to $300");
            a.appendChild(linkText);
            a.href = "https://survey.zohopublic.com/zs/oiBUVB";
            a.rel = "nofollow";
