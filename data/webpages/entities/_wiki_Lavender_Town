            //    a.id = 'amazon-related';
            //    var linkText = document.createTextNode("Get Notified about Pokémon the Series: Sun & Moon Season 20 Premiere");
            //    a.appendChild(linkText);
            //    //a.title = "Buy related Pokémon products";
            //    a.href = "https://goo.gl/qGbju4";
