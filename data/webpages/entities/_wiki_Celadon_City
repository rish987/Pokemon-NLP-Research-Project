            amzSearchTerm = "";
            if (firstHeading.textContent != null && firstHeading.textContent.includes('(')) {
                amzSearchTerm = firstHeading.textContent.slice(0, firstHeading.textContent.indexOf('(')) + firstHeading.textContent.slice(firstHeading.textContent.indexOf(')') + 1).trim();
            }
            else {
                amzSearchTerm = firstHeading.textContent.trim();
            }

            tagCode = '';
            if (country == 'us') {
                tagCode = 'bulbapedia-20';
            }
            else {
                tagCode = 'bulbapedia-21';
            }

            var a = document.createElement('a');
            a.id = 'amazon-related';
            var linkText = document.createTextNode("Bulba Readership Survey - Win up to $300");
            a.appendChild(linkText);
            a.href = "https://survey.zohopublic.com/zs/oiBUVB";
            a.rel = "nofollow";
