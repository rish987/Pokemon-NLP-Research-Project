    function insertSortableAd(adId, adInsertionPoint) {
        if (document.querySelector('[data-ad-name=' + adId + ']') == null) {
            var adElement = document.createElement('div');
            adElement.setAttribute('data-ad-name', adId);
            adElement.setAttribute('data-ad-size', '728x90');
            adInsertionPoint.parentNode.insertBefore(adElement, adInsertionPoint.nextSibling);
        }
    }

